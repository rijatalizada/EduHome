@model HomeNoticePage

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-md-6 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">PostDate : @Model.PostDate.ToString("dd, MMM yyyy")</h4>
                        <p>
                            Description : @Html.Raw(Model.Description)
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

