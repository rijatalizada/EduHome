        <!-- Event Area Start -->
        <div class="event-area one text-center pt-140 pb-150">
            <div class="container">
                <div class="row">
                     <div class="col-12">
                        <div class="section-title">
                            <img src="~/assets/img/icon/section.png" alt="section-title">
                            <h2>UPCOMMING EVENTS</h2>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6">
                        @foreach (var item in Model.Event)
                        {
                            if (@item.Id > 6 ) break;;
                            <div class="single-event mb-35">
                                <div class="event-date">
                                    <h3><a asp-controller="Event" asp-action="Detail" asp-route-id="@item.Id" asp-route-categoryId="@item.EventCategories.FirstOrDefault().CategoryId">@item.Date.ToString("dd") <span>@item.Date.ToString("MMM")</span></a></h3>
                                </div>
                                <div class="event-content text-start">
                                    <div class="event-content-left">
                                        <h4><a asp-controller="Event" asp-action="Detail" asp-route-id="@item.Id" asp-route-categoryId="@item.EventCategories.FirstOrDefault().CategoryId">@item.Name.ToUpper()</a></h4>
                                        <ul>
                                            <li><i class="fa fa-clock-o"></i>@item.Time</li>
                                            <li><i class="fa fa-map-marker"></i>@item.Venue</li>
                                        </ul>
                                    </div>
                                    <div class="event-content-right">
                                        <a class="default-btn" asp-controller="Event" asp-action="Detail" asp-route-id="@item.Id" asp-route-categoryId="@item.EventCategories.FirstOrDefault().CategoryId">join now</a>
                                    </div>
                                </div>
                            </div>
                        }
                        

                    </div>
                    <div class="col-lg-6 pt-5 pt-lg-0">
